# Path: /iam/.env
# ===================================================================
# IAM STACK ENVIRONMENT
# ===================================================================

# --- Service Versions (Pinned) ---
KEYCLOAK_VERSION=26.3
POSTGRES_VERSION=latest

# --- Keycloak Database Credentials ---
POSTGRES_DB="keycloak"
POSTGRES_USER="keycloak"
POSTGRES_PASSWORD="GenerateAStrongUniquePasswordForKeycloakDB"

# --- Keycloak Admin Credentials ---
KEYCLOAK_ADMIN_USER="admin"
KEYCLOAK_ADMIN_PASSWORD="GenerateAStrongUniquePasswordForKeycloakAdmin"

# --- Keycloak Subdomain ---
KEYCLOAK_SUBDOMAIN="auth"
