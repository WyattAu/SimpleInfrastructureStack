# Path: /iam/.env
# ===================================================================
# IAM STACK ENVIRONMENT
# ===================================================================

# --- Service Versions ---
# Use a specific Bitnami Keycloak version tag
KEYCLOAK_VERSION=24.0.2
POSTGRES_VERSION=16.2-alpine

# --- Keycloak Database Credentials ---
KEYCLOAK_DATABASE_NAME="keycloak"
KEYCLOAK_DATABASE_USER="keycloak"
KEYCLOAK_DATABASE_PASSWORD="GenerateAStrongUniquePasswordForKeycloakDB"

# --- Keycloak Admin Credentials ---
KEYCLOAK_CREATE_ADMIN_USER="yes"
KEYCLOAK_ADMIN_USER="admin"
KEYCLOAK_ADMIN_PASSWORD="GenerateAStrongUniquePasswordForKeycloakAdmin"
